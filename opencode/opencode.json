  "model": "opencode/minimax-m2.5-free",
  "default_agent": "interactive",
  "plugin": [
    "opencode-antigravity-auth@latest",
    "opencode-qwencode-auth",
    "opencode-openai-codex-auth",
    "cc-safety-net",
    "@azumag/opencode-rate-limit-fallback",
    "@ramtinj95/opencode-tokenscope"
  ],
  "agent": {
    "minimal": {
      "description": "Matter-of-fact assistant",
      "mode": "primary",
      "prompt": "{file:/home/dzack/ai/prompts/interactive_agents/minimal.md}"
    },
    "interactive": {
      "description": "Default collaborative agent - handles trivial to complex tasks, user-in-the-loop",
      "mode": "primary",
      "prompt": "{file:/home/dzack/ai/prompts/interactive_agents/interactive.md}"
    },
    "executor": {
      "description": "Executes plan with batch-first parallelism - spawns subagents for parallel implementation",
      "mode": "primary",
      "prompt": "{file:/home/dzack/ai/prompts/interactive_agents/executor.md}"
    },
    "artifact-searcher": {
      "description": "Searches serena memories for relevant past work, patterns, and lessons learned",
      "mode": "subagent",
      "prompt": "{file:/home/dzack/ai/prompts/subagents/artifact-searcher.md}"
    },
    "code-compliance-reviewer": {
      "description": "Reviews generated code against project constraints and reports violations",
