{
  "$schema": "https://opencode.ai/config.json",
  "mcp": {
    "serena": {
      "command": [
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--project-from-cwd"
      ],
      "type": "local"
    },
    "morph": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@morphllm/morphmcp"
      ]
    },
    "kindly": {
      "type": "local",
      "command": [
        "uvx",
        "--from",
        "git+https://github.com/Shelpuk-AI-Technology-Consulting/kindly-web-search-mcp-server",
        "kindly-web-search-mcp-server",
        "start-mcp-server"
      ]
    },
    "context7": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "cut-copy-paste-mcp": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@fastmcp-me/cut-copy-paste-mcp"
      ]
    }
  },
